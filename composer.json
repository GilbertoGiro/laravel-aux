{
  "name": "innovareti/laravel-aux",
  "description": "",
  "homepage": "https://github.com/innovareti/laravel-aux",
  "license": "MIT",
  "authors": [
    {
      "name": "Gilberto Giro Resende",
      "email": "gilberto.giro.resende@gmail.com",
      "homepage": "https://github.com/GilbertoGiro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",    
    "doctrine/dbal": "2.7",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^8.0",
    "laravel/tinker": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelAux\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelAux\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
        "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.11"
    },
    "laravel": {
      "providers": [
        "LaravelAux\\LaravelAuxServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.19",
    "phpunit/phpunit": "^9.5"
  }
}
